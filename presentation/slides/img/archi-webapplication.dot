digraph ArchiWebApp {
	rankdir = LR
	ranksep = 0.4
	nodesep = 0.8
	node [ shape = none, fontsize = 30 ]
	edge [ arrowhead = none, arrowtail = none ]

	WebClient1 [ label =
		<<table border="0"><tr><td>
		<img src="icon-computer.png" />
		</td></tr></table>> ]
	WebClient2 [ label =
		<<table border="0"><tr><td>
		<img src="icon-computer.png" />
		</td></tr></table>> ]
	RouterLAN [ label =
		<<table border="0"><tr><td>
		<img src="icon-wifi.png" />
		</td></tr></table>> ]
	Internet [ label =
		<<table border="0"><tr><td>
		<img src="icon-cloud.png" />
		</td></tr></table>> ]
	FirewallExt [ label =
		<<table border="0"><tr><td>
		<img src="icon-firewall.png" />
		</td></tr></table>> ]
	{
		DMZ [ shape = circle ]
		WebServer [ label =
			<<table border="0"><tr><td>
			<img src="icon-server.png" />
			</td></tr><tr><td>Web server
			</td></tr></table>> ]
		rank = same
	}
	Firewall [ label =
		<<table border="0"><tr><td>
		<img src="icon-firewall.png" />
		</td></tr></table>> ]
	{
		RouterServer [ shape = point ]
		WebApplication [ label =
			<<table border="0"><tr><td>
			<img src="icon-server.png" />
			</td></tr><tr><td>Web application<br />server
			</td></tr></table>> ]
		rank = same
	}
	LocalClient [ label =
		<<table border="0"><tr><td>
		<img src="icon-computer.png" />
		</td></tr></table>> ]
	Database [ label =
		<<table border="0"><tr><td>
		<img src="icon-database.png" />
		</td></tr></table>> ]

	{ WebClient1 WebClient2 } -> RouterLAN [ style = dotted ]
	RouterServer -> { LocalClient }
	RouterLAN -> Internet -> FirewallExt -> DMZ
	DMZ -> WebServer
	DMZ -> Firewall -> RouterServer
	RouterServer -> { WebApplication Database }
}
